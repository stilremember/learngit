#password-edit-container .form-label { float: left; width: 150px; text-align: right; font-size: 14px; color: #666; padding-right: 10px; }
#password-edit-container .txt03 { width: 350px; line-height: 16px; padding: 9px 10px; border: 1px solid #cccccc; float: none; font-size: 14px; font-family: arial,"宋体"; color: #999; overflow: hidden; border-radius: 3px; -moz-border-radius: 3px; -webkit-border-radius: 3px; }