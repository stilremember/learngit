.schedule_wrapper { overflow-y: auto; }
#schedule li { width: 100%; white-space: nowrap; overflow: hidden; text-overflow: ellipsis; color:#fff; }
#schedule li.schedule_service span { float: none; color: #F60; }

/* 首页右键菜单样式 */
#homePage-context-menu { display: none; }

